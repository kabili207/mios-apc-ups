<?xml version="1.0"?>
<scpd xmlns="urn:schemas-upnp-org:service-1-0">
  <specVersion>
    <major>1</major>
    <minor>0</minor>
  </specVersion>
  <serviceStateTable>
    <stateVariable sendEvents="no">
      <name>URL</name>
      <dataType>string</dataType>
      <defaultValue>Enter the URL to for the APC stats CGI</defaultValue>
    </stateVariable>
    <stateVariable sendEvents="no">
      <name>Period</name>
      <dataType>ui4</dataType>
      <defaultValue>1800</defaultValue>
    </stateVariable>
    <stateVariable sendEvents="yes">
      <name>OnBattery</name>
      <dataType>boolean</dataType>
    </stateVariable>
    <stateVariable sendEvents="no">
      <name>Period</name>
      <dataType>ui4</dataType>
      <defaultValue>1800</defaultValue>
    </stateVariable>
    <stateVariable sendEvents="no">
      <name>NominalWattage</name>
      <dataType>ui4</dataType>
    </stateVariable>
    <stateVariable sendEvents="yes">
      <name>LineVoltage</name>
      <dataType>ui4</dataType>
    </stateVariable>
    <stateVariable sendEvents="yes">
      <name>TimeRemaining</name>
      <dataType>ui4</dataType>
    </stateVariable>
  </serviceStateTable>
  <actionList>
    <action>
      <name>SetURL</name>
      <argumentList>
		<argument>
		  <name>url</name>
		  <direction>in</direction>
			  <relatedStateVariable>URL</relatedStateVariable>
		</argument>
      </argumentList>
    </action>
    <action>
      <name>SetPeriod</name>
      <argumentList>
		<argument>
		  <name>period</name>
		  <direction>in</direction>
			  <relatedStateVariable>Period</relatedStateVariable>
		</argument>
      </argumentList>
    </action>
    <action>
      <name>SetNominalWattage</name>
      <argumentList>
		<argument>
		  <name>wattage</name>
		  <direction>in</direction>
			  <relatedStateVariable>NominalWattage</relatedStateVariable>
		</argument>
      </argumentList>
    </action>
  </actionList>
</scpd>
